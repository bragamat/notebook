<div class="basic-grey">
<p id="notice"><%= notice %></p>

<h1> <%= "#{t('messages.listing')} #{@phones.model.model_name.human(count: @phones.count)}" %></h1>


<table>
  <thead>
    <tr>
      <th><%= @phones.model.human_attribute_name ("phone") %></th>
      <th><%= @phones.model.human_attribute_name ("contact") %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phones.each do |phone| %>
      <tr>
        <td><%= phone.phone %></td>
        <td><%= phone.contact.name %></td>
        <td><%= link_to 'Show', phone %></td>
        <td><%= link_to 'Edit', edit_phone_path(phone) %></td>
        <td><%= link_to 'Destroy', phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('buttons.new_phone'), new_phone_path, class: "button" %>
<%= link_to t('buttons.back'), home_index_path, class: "button" %>
</div>